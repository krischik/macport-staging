local User_Data="${HOME}/.free42"
local System_Data="/opt/local/share/free42";

if test ! -d "${User_Data}"; then
    mkdir "${User_Data}";
    ln -s "/opt/local/share/free/"*  "${User_Data}/"
fi;

/opt/local/bin/free42dec &

